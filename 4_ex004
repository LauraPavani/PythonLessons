i= input('Insert something: ')
pprint('The primitive type is: ', type(i))rint(type(i))
print('Is it a number? {}'.format(i.isnumeric()))
print('Is it alphanumeric? {}'.format(i.isalnum()))
print('Is it capitalized? {}'.format(i.islower()))
print('Is it decimal? {}'.format(i.isdecimal()))
print('Is it a title? {}'.format(i.istitle()))
print('Is it space? {}'.format(i.isspace()))
print('Is it alphabetic? {}'.format(i.isalpha()))
